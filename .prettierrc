{
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "consistent",
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "overrides": [
      {
          "files": "*.ts",
          "options": {
              "tabWidth": 2
          }
      },
      {
          "files": "*.json",
          "options": {
              "singleQuote": false
          }
      }
  ]
}
